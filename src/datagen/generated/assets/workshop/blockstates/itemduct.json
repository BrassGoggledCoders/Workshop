{
  "multipart": [
    {
      "apply": {
        "model": "workshop:block/itemduct_center"
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "workshop:block/itemduct_side",
        "x": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "facing": "down"
      },
      "apply": {
        "model": "workshop:block/itemduct_side_output",
        "x": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "workshop:block/itemduct_side",
        "x": -90,
        "uvlock": true
      }
    },
    {
      "when": {
        "facing": "up"
      },
      "apply": {
        "model": "workshop:block/itemduct_side_output",
        "x": -90,
        "uvlock": true
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "workshop:block/itemduct_side",
        "uvlock": true
      }
    },
    {
      "when": {
        "facing": "north"
      },
      "apply": {
        "model": "workshop:block/itemduct_side_output",
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "workshop:block/itemduct_side",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "facing": "south"
      },
      "apply": {
        "model": "workshop:block/itemduct_side_output",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "workshop:block/itemduct_side",
        "y": 270,
        "uvlock": true
      }
    },
    {
      "when": {
        "facing": "west"
      },
      "apply": {
        "model": "workshop:block/itemduct_side_output",
        "y": 270,
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "workshop:block/itemduct_side",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "facing": "east"
      },
      "apply": {
        "model": "workshop:block/itemduct_side_output",
        "y": 90,
        "uvlock": true
      }
    }
  ]
}